{
  "name": "@pixelfederation/gh-action-checkout",
  "version": "0.1.0",
  "description": "git clone action",
  "repository": "https://github.com/gh-action-checkout/",
  "author": "Tomas Hulata <thulata@pixelfederation.com>",
  "license": "MIT",
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "build": "esbuild src/checkout/main.ts --bundle --outdir=. --target=node18 --platform=node"
  },
  "dependencies": {
    "@actions/core": "v1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/io": "1.1.3",
    "action-docs": "^2.1.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-config-prettier": "9.1.0",
    "@typescript-eslint/parser": "6.21.0",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  }
}
